<% include head %>
<div class="row">
     <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" style="padding-top:30px">

       <% if (message.length > 0) { %>
         <div class="alert alert-danger col-sm-12" ><%= message %></div>
       <% } else {%>
         <div class="panel panel-info">
           <div class="panel-heading">
             <h3 class="panel-title"><%=data[0].first_name%>, <%=data[0].last_name%></h3>
           </div>
         </div>
           <div >
             <div >
               <div  align="center"> <img alt="User Pic" src="/static/public/images/upload_images/<%=data[0].image%>" class="img-responsive"> </div>
               <figcaption>Image name: <%=data[0].image%></figcaption>
               <br>
               <div >
                 <table class="table table-user-information">
                   <tbody>
                     <th>Description</th>
                     <tr>
                       <td>
                         <%= data[0].description %>

                       </td>
                       </tr>


                   </tbody>
                 </table>
               </br></br>
                  &nbsp;
                 <button class="btn btn-info"  type='submit'> <a href="\upload">Go Back</a> </button>
                 <button class="btn btn-info" style="float:right" type='submit'> <a href="\project">Do Search</a> </button>
               </div>
             </div>
           </div>
         </div>
       <% } %>
       </div>
     </div>
